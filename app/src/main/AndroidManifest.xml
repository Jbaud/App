<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jules.hi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/beta_icone"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".PageLoading"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.Translucent">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--
             ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".PageHome"
            android:label="@string/title_activity_display_image"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".PageAbout"
            android:label="@string/title_activity_about"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".PageShows"
            android:label="@string/title_activity_my_shows"
            android:theme="@style/Theme.AppCompat.NoActionBar"></activity>
    </application>

</manifest>
